<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp14.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp14"
    xmlns:modules="clr-namespace:MauiApp14.PersonelModule"
    Title="MauiApp14"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <FlyoutItem Title="pers">
        <Tab>
            <ShellContent
                Title="Personel Liste"
                ContentTemplate="{DataTemplate modules:PersonelListView}"
                Route="PersonelListPage" />


            <ShellContent
                Title="Adres"
                ContentTemplate="{DataTemplate modules:Adres}"
                Route="Adres" />


        </Tab>
    </FlyoutItem>



</Shell>
